<table>
  <thead>
    <th>Ranking</th>
    <th>Name</th>
    <th>Upvote</th>
  </thead>
  <tbody>
    <% @books.each do |book| %>
      <tr>
        <td>Ranked: <%= book.rank%></td>
        <td><%= link_to book.title, book_path(book.id) %></td>
        <td><%= button_to 'Upvote', upvote_book_path(book.id) %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<%= button_to 'View All Media', root_path, :method => :get %> <%= button_to 'Add A Book', new_book_path %>
