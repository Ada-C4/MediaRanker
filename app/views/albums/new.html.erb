<h2> New Album </h2>
  <%= form_for @album, action: 'create', method: :post do |f| %>
<fieldset>
    <%= f.label :name %>
    <%= f.text_field :name %>
    <br>
    <%= f.label :artist %>
    <%= f.text_field :artist %>
    <br>
    <%= f.label :description %>
    <%= f.text_area :description %>
    <br>
  </fieldset>
  <%= f.submit("Save") %>
  <% end %>
