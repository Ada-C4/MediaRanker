<table class="table table-hover">
  <tr>
    <th>
      Ranking
    </th>
    <th>
      Name
    </th>
    <th>
      Upvote
    </th>
  </tr>
  <% @albums.each do |album| %>
    <tr>
      <td>
        Ranked: <%= album.ranked %>
      </td>
      <td>
        <%= link_to album.name, album_path(album) %>
      </td>
      <td>
        <%= button_to "Upvote", album_upvote_path(album), method: :patch, class: "btn btn-default" %>
      </td>
    </tr>
  <% end %>
</table>
<%= link_to "View All Media", root_path, method: :get, class: "btn btn-default" %>
 <%= link_to "Add an Album", new_album_path, method: :get, class: "btn btn-default" %>
