<table>
  <th>Ranking</th>
  <th>Name  </th>
  <th> Upvote </th>
  <% @albums.each do |album| %>
    <tr>
      <td>Ranked: <%= album.rank %> </td>
      <td> <%= album.name %> </td>
      <td> <%= button_to "Upvote", { action: "update" , class: "upvote", id: album.id}, method: "patch" %> </td>
    </tr>
  <% end %>
</table>
