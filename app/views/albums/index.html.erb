<table class="table table-hover">
  <tbody>
    <tr>
      <th>Ranking</th>
      <th>Name</th>
      <th>Upvote</th>
    </tr>
    <% @albums.each do |album| %>
      <tr>
        <td><%= album.rank %></td>
        <td><%= link_to album.name, album_path(album.id) %></td>
      </tr>
    <% end %>
  </tbody>
</table>
