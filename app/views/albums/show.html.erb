<h1> <%= @album.name %> <small> Recorded by: <%= @album.artist %></small> </h1>
<h4>Ranked: <%= @album.rank %> </h4>
<h5><%= @album.description %> </h5>
<%= button_to 'Upvote', album_upvote_path(@album.id), class: "btn btn-primary" %>



<%= link_to 'EDIT', edit_album_path, class: "btn btn-default" %>
<%= link_to 'DELETE',  { action: :destroy, id: @album.id }, method: :delete, data: { confirm: 'Delete album, For Real?' }, class: "btn btn-danger" %>
<%= link_to 'View All albums', albums_path %>
