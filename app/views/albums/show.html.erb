<h1> <%= @album.name %> </h1> <h2>Recorded by: <%= @album.artist %> </h2>
<h3>Ranked: <%= @album.rank %> </h3>
<h4><%= @album.description %> </h4>
<%= button_to 'Upvote', album_upvote_path(@album.id) %>



<%= link_to 'EDIT', edit_album_path %>
<%= link_to 'DELETE',  { action: :destroy, id: @album.id }, method: :delete, data: { confirm: 'Delete album, For Real?' } %>
<%= link_to 'View All albums', albums_path %>
