<div class="col-lg-12">
  <table class="table table-hover">
  	<tbody>
      <tr>
        <th>Ranking</th>
        <th>Name</th>
        <th>Upvote</th>
      </tr>
			<tr>
	      <% media.each do |m| %>
	      	<td>Ranked: <%= m.rank %></td>
	      	<td><%= link_to m.name, polymorphic_path(m) %></td>
	      	<td><%= button_to "Upvote", polymorphic_path([:vote, m]), method: "post", class: "btn btn-default"%></td>
	      <% end %>
			</tr>
		</tbody>
	</table>

	<%= link_to "View All Media", root_path, method: "get", class: "btn btn-default" %>
	<%= link_to "Add #{sing_name.capitalize}", new_polymorphic_path(sing_name), method: "get", class: "btn btn-default" %>
</div>