<table>
  <thead>
    <tr>
      <th>
        Ranking
      </th>
      <th>
        Name
      </th>
      <th>
        Upvote
      </th>
    </tr>
  </thead>
  <% @movies.each do |movie| %>
  <tr>
    <td>
      Ranked: <%= movie.votes %>
    </td>
    <td>
      <%= link_to movie.name, movie_path(movie.id) %>
    </td>
    <td>
      <%= button_to "Upvote" %>
    </td>
  </tr>
  <% end %>
</table>

<%= link_to "View All Media", "/" %>
<%= link_to "Add a Movie", new_movie_path %>
