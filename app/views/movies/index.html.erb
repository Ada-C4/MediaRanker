<div class="col-lg-12">
  <table class="table table-hover">
    <tbody>
      <tr>
        <th> Ranking </th>
        <th> Name </th>
        <th> Upvote </th>
      </tr>
    <% @movies.each do |movie| %>
      <tr>
      <td><%= movie.ranking %></td>
      <td><%= link_to movie.title, movie_path(movie.id) %></td>
      <td><%= button_to "Upvote", upvote_movie_path(movie.id) %></td>
    <% end %>
      </tr>
    </tbody>
  </table>
<%= link_to "View all media", root_url, class: "btn btn-default" %>  <%= link_to "Add a movie", new_movie_path, class: "btn btn-default" %>
</div>
