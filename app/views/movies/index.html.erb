<table>
  <tr>
    <th>Ranking</th>
    <th>Name</th>
    <th>Upvote</th>
  </tr>
  <% @movies.each do |movie| %>
  <tr>
    <td>
      Ranked: <%= movie.ranking %>
    </td>
    <td>
      <%= link_to movie.name, movie_path(id: movie.id) %>
    </td>
    <td>
      <%= button_to "Upvote", movie_path(id: movie.id) %>
    </td>
  </tr>
  <% end %>
</table>

<%= link_to "View All Media", "/" %>
<%= link_to "Add a Movie", new_movie_path %>
