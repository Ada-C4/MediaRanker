<table class="table table-hover">
  <thead>
  <tr>
    <th>Ranking</th>
    <th>Name</th>
    <th>Upvote</th>
  </tr>
</thead>
  <tbody>

    <% @model.each do |model| %>
      <tr>
        <td> <%= model.votes %></td>
        <td> <%= link_to model.title, model %></td>
        <td> <%= button_to 'Upvote', polymorphic_path([:upvote, model]), method: :post, class:"btn btn-default", role:"button"%>
</td>
      </tr>
    <% end %>
  </tbody>
</table>
<%= link_to "View All Media", root_path, class:"btn btn-default", role:"button" %>
<%= link_to @add_button, @path, class:"btn btn-default", role:"button" %>
