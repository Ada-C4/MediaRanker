
<table class='table'>
  <tr>
    <th>Ranking</th>
    <th>Name</th>
    <th>Upvote</th>
  </tr>
  <% @media.each do |media| %>
    <tr>
      <td>Ranked: <%= media.votes %></td>
      <td><%= link_to "#{media.name}", polymorphic_path(media) %></td> 
      <td>
        <%= button_to 'Upvote', polymorphic_path([:upvote, media]), :method => :patch, class: 'btn btn-info' %>
      </td>
    </tr>
  <% end %>
</table>

