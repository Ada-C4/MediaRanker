<div class="row">
  <div class="col-lg-12">
    <table class="table table-hover">
      <tr>
        <th>Ranking</th>
        <th>Name</th>
        <th>Upvote</th>
      </tr>
      <% items.each do |item| %>
      <tr>
        <td>Ranked: <%= item.ranking %></td>
        <td><%= link_to item.name, polymorphic_path(item) %></td>
        <td><%= button_to "Upvote", polymorphic_path([:upvote, item]), class: "btn btn-default" %></td>
      </tr>
      <% end %>
    </table>
  </div>
</div>

<%= link_to "View All Media", "/" , class: "btn btn-default"%>
<%= link_to "Add a #{items.first.class}", new_polymorphic_path(items.first.class), class: "btn btn-default" %>
