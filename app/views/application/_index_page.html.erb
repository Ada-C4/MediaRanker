<table>
  <tr>
    <th>Ranking</th>
    <th>Name</th>
    <th>Upvote</th>
  </tr>
  <% items.each do |item| %>
  <tr>
    <td>
      Ranked: <%= item.ranking %>
    </td>
    <td>
      <%= link_to item.name, polymorphic_path(item) %>
    </td>
    <td>
      <%= button_to "Upvote", polymorphic_path([:upvote, item]) %>
    </td>
  </tr>
  <% end %>
</table>

<%= link_to "View All Media", "/" %>
<%= link_to "Add a #{items.first.class}", new_polymorphic_path(items.first.class) %>
