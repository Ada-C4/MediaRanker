<table>
  <tr>
    <th>
      Ranking
    </th>
    <th>
      Name
    </th>
    <th>
      Upvote
    </th>
  </tr>
  <% @books.each do |book| %>
    <tr>
      <td>
        Ranked: <%= book.ranked %>
      </td>
      <td>
        <%= link_to book.name, book_path(book) %>
      </td>
      <td>
        Upvote Button Will Go Here
      </td>
    </tr>
  <% end %>
</table>
