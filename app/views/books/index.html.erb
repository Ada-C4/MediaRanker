<table>
  <tbody>
    <tr>
      <th>Ranking</th>
      <th>Name</th>
      <th>Upvote</th>
    </tr>
    <% @books.each do |book| %>
      <tr>
        <td><%= book.upvotes %></td>
        <td><%= link_to book.name, book_path(book.id) %></td>
        <td>[Upvote Button]</td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to "View All Media", root_path %>
<%= link_to "Add a Book", new_book_path %>
